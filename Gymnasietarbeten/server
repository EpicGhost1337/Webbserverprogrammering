CREATE TABLE `products` (
 `id` int(11) NOT NULL,
 `p_name` varchar(255) NOT NULL,
 `image` varchar(255) NOT NULL,
 `price` double(10,2) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

